{
  "name": "nx-fullstack-starter",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "serve:apps:frontend:web": "nx serve frontend-web --disable-host-check",
    "serve:apps:backend": "export NODE_ENV=development && export NODE_TLS_REJECT_UNAUTHORIZED='0' && nx serve backend",
    "build:apps:frontend:web": "nx build frontend-web",
    "build:apps:backend": "nx build backend",
    "test:apps:frontend": "run-p test:apps:frontend:*",
    "test:apps:frontend:web": "nx run frontend-web:test --code-coverage",
    "test:libs:fronend": "run-p test:libs:frontend:*",
    "test:libs:frontend:auth": "nx test frontend-auth --code-coverage",
    "test:libs:frontend:layout": "nx test frontend-layout --code-coverage",
    "test:libs:frontend:ui": "nx test frontend-ui --code-coverage",
    "docker:start": "run-p docker:start:*",
    "docker:start:nginx": "docker-compose -f ./docker/nginx/docker-compose.yml up --build -d",
    "docker:start:mongo": "docker-compose -f ./docker/mongo/docker-compose.yml up --build -d",
    "docker:stop": "run-p docker:stop:*",
    "docker:stop:nginx": "docker-compose -f ./docker/nginx/docker-compose.yml down",
    "docker:stop:mongo": "docker-compose -f ./docker/mongo/docker-compose.yml down",
    "docker:restart": "run-p docker:restart:*",
    "docker:restart:mongo": "run-s docker:stop:mongo docker:start:mongo",
    "docker:restart:nginx": "run-s docker:stop:nginx docker:start:nginx",
    "docker:logs": "run-p docker:logs:*",
    "docker:logs:nginx": "docker-compose -f ./docker/nginx/docker-compose.yml logs -f -t",
    "docker:logs:mongo": "docker-compose -f ./docker/mongo/docker-compose.yml logs -f -t",
    "postinstall": "ngcc --properties es2020 browser module main"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.1",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "14.1.1",
    "@angular/language-service": "14.1.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/angular": "14.5.4",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nest": "14.5.4",
    "@nrwl/node": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "autoprefixer": "^10.4.0",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "mongodb-memory-server": "^8.8.0",
    "ng-mocks": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "nx": "14.5.4",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "supertest": "^6.2.4",
    "tailwindcss": "^3.0.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "dependencies": {
    "@angular/animations": "14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "14.1.1",
    "@angular/compiler": "14.1.1",
    "@angular/core": "14.1.1",
    "@angular/forms": "14.1.1",
    "@angular/platform-browser": "14.1.1",
    "@angular/platform-browser-dynamic": "14.1.1",
    "@angular/router": "14.1.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
